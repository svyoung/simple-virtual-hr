<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <title>EmployeeDirectory</title>

        <!-- stylesheet -->
        <link rel="stylesheet" type="text/css" href="/css/main.css">
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

        <!-- js -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.5.0/react.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.5.0/react-dom.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
        <script src="/js/main.js" type="text/babel"></script>
        <script src="/js/js.js" type="text/javascript"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.16.1/axios.min.js"></script>

    </head>
    <body>
    <div id="mainheader"></div>
    <div id="main"></div>
    <div class="container">
        <div class="row">
            <div id="site-art" class="col-xs-6 col-md-4">
                Directory
            </div>
            <div id="employee-profile-active-display" class="col-md-6 bs-right-override" style="display: none"></div>
            <div id="site-content" class="col-xs-12 col-md-8 bs-left-override">
                <h1>Employees &nbsp;&nbsp;<a class="employee-add">+</a></h1> 
                <% for(var i in employees) { %>
                    <h3 data-alphabet="<%= i %>"><%= i %></h3>
                    <div class="employee-list">
                        <% for(var j = 0; j < employees[i].length; j++) { %>
                        <div class="employee-cell">
                            <a class="employee_click" data-emp-id="<%= employees[i][j].id %>"><%= employees[i][j].name %></a>
                        </div>
                        <% } %>     
                    </div>
                <% } %>               
            </div>
            
        </div>
    </div>
    
    <% if(typeof authenticated != "undefined") { %>
        <script src="/js/authenticated.js" type="text/babel"></script>
    <% } %> 
    </body>
</html>
